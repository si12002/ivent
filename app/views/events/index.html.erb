<h1>イベント一覧</h1>

<table>
  <thead>
    <tr>
      <th>No</th>
      <th>イベント名</th>
      <th>日時</th>
      <!-- <th>Dtend</th> -->
      <th>場所</th>
      <!-- <th>Lat</th> -->
      <!-- <th>Lng</th> -->
      <!-- <th>Summary</th> -->
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.id %></td>
        <td><%= event.title %></td>
        <td><%= event.dtstart %></td>
       <!-- <td><%= event.dtend %></td> -->
        <td><%= event.location %></td>
        <!-- <td><%= event.lat %></td> -->
        <!-- <td><%= event.lng %></td> -->
        <!-- <td><%= event.summary %></td> -->
        <td><%= link_to '詳細', event %></td>
        <!-- <td><%= link_to '編集', edit_event_path(event) %></td>
        <td><%= link_to '削除', event, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>

<br>

